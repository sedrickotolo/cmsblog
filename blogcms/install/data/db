CREATE TABLE `category` (
  `id` int(11) NOT NULL,
  `url` varchar(255) NOT NULL,
  `title` varchar(255) NOT NULL,
  `icon` varchar(30) NOT NULL,
  `name` varchar(255) NOT NULL,
  `slider` text NOT NULL,
  `status` tinyint(1) NOT NULL,
  `parent` int(11) NOT NULL,
  `header` varchar(255) NOT NULL,
  `images` text NOT NULL,
  `content` text NOT NULL,
  `position` int(11) NOT NULL,
  `elements` text NOT NULL,
  `keywords` text NOT NULL,
  `description` varchar(200) NOT NULL
) ENGINE=InnoDB DEFAULT CHARSET=utf8;
###
CREATE TABLE `chat` (
  `id` int(11) NOT NULL,
  `time` timestamp NOT NULL,
  `user` smallint(6) NOT NULL,
  `message` text NOT NULL,
  `position` int(11) NOT NULL
) ENGINE=InnoDB DEFAULT CHARSET=utf8;
###
CREATE TABLE `comments` (
  `id` int(11) NOT NULL,
  `ip` varchar(40) NOT NULL,
  `user` smallint(6) NOT NULL,
  `time` timestamp NOT NULL,
  `reply` int(11) NOT NULL,
  `name` varchar(255) NOT NULL,
  `email` varchar(255) NOT NULL,
  `status` tinyint(1) NOT NULL,
  `content` int(11) NOT NULL,
  `position` int(11) NOT NULL,
  `comment` text NOT NULL
) ENGINE=InnoDB DEFAULT CHARSET=utf8;
###
CREATE TABLE `content` (
  `id` int(11) NOT NULL,
  `url` varchar(255) NOT NULL,
  `user` smallint(6) NOT NULL,
  `title` varchar(255) NOT NULL,
  `tags` varchar(255) NOT NULL,
  `time` timestamp NOT NULL,
  `form` text NOT NULL,
  `status` tinyint(1) NOT NULL,
  `rating` smallint(6) NOT NULL,
  `views` smallint(11) NOT NULL,
  `video` text NOT NULL,
  `slider` text NOT NULL,
  `header` varchar(255) NOT NULL,
  `images` text NOT NULL,
  `position` int(11) NOT NULL,
  `fullentry` text NOT NULL,
  `category` varchar(255) NOT NULL,
  `elements` text NOT NULL,
  `keywords` text NOT NULL,
  `shortentry` text NOT NULL,
  `description` varchar(200) NOT NULL
) ENGINE=InnoDB DEFAULT CHARSET=utf8;
###
CREATE TABLE `formdata` (
  `id` int(11) NOT NULL,
  `json` text NOT NULL,
  `time` timestamp NOT NULL,
  `page` varchar(255) NOT NULL,
  `status` tinyint(1) NOT NULL,
  `position` int(11) NOT NULL
) ENGINE=InnoDB DEFAULT CHARSET=utf8;
###
CREATE TABLE `logs` (
  `id` int(11) NOT NULL,
  `time` timestamp NOT NULL,
  `user` smallint(6) NOT NULL,
  `event` text NOT NULL,
  `position` int(11) NOT NULL
) ENGINE=InnoDB DEFAULT CHARSET=utf8;
###
CREATE TABLE `messages` (
  `id` int(11) NOT NULL,
  `time` timestamp NOT NULL,
  `user` smallint(6) NOT NULL,
  `status` tinyint(1) NOT NULL,
  `subject` varchar(255) NOT NULL,
  `message` text NOT NULL,
  `position` int(11) NOT NULL,
  `recipient` smallint(6) NOT NULL
) ENGINE=InnoDB DEFAULT CHARSET=utf8;
###
CREATE TABLE `pages` (
  `id` int(11) NOT NULL,
  `url` varchar(255) NOT NULL,
  `icon` varchar(255) NOT NULL,
  `title` varchar(255) NOT NULL,
  `type` tinyint(4) NOT NULL,
  `form` text NOT NULL,
  `blog` mediumtext NOT NULL,
  `name` varchar(255) NOT NULL,
  `class` varchar(255) NOT NULL,
  `video` text NOT NULL,
  `slider` text NOT NULL,
  `status` tinyint(1) NOT NULL,
  `parent` int(11) NOT NULL,
  `header` varchar(255) NOT NULL,
  `images` text NOT NULL,
  `position` int(11) NOT NULL,
  `content` text NOT NULL,
  `elements` text NOT NULL,
  `keywords` text NOT NULL,
  `description` varchar(200) NOT NULL
) ENGINE=InnoDB DEFAULT CHARSET=utf8;
###
CREATE TABLE `statistics` (
  `id` int(11) NOT NULL,
  `total` int(11) NOT NULL,
  `period` varchar(255) NOT NULL,
  `uniques` int(11) NOT NULL
) ENGINE=InnoDB DEFAULT CHARSET=utf8;
###
CREATE TABLE `subscriptions` (
  `id` int(11) NOT NULL,
  `time` timestamp NOT NULL,
  `email` varchar(255) NOT NULL,
  `status` tinyint(1) NOT NULL,
  `position` int(11) NOT NULL
) ENGINE=InnoDB DEFAULT CHARSET=utf8;
###
CREATE TABLE `tags` (
  `id` int(11) NOT NULL,
  `time` timestamp NOT NULL,
  `name` varchar(255) NOT NULL,
  `position` int(11) NOT NULL
) ENGINE=InnoDB DEFAULT CHARSET=utf8;
###
CREATE TABLE `users` (
  `id` int(11) NOT NULL,
  `tin` varchar(255) NOT NULL,
  `lost` varchar(255) NOT NULL,
  `role` smallint(6) NOT NULL,
  `hash` varchar(32) NOT NULL,
  `time` timestamp NOT NULL,
  `login` varchar(32) NOT NULL,
  `name` varchar(255) NOT NULL,
  `email` varchar(255) NOT NULL,
  `status` tinyint(1) NOT NULL,
  `phone` varchar(255) NOT NULL,
  `address` varchar(255) NOT NULL,
  `password` varchar(32) NOT NULL,
  `organization` varchar(255) NOT NULL
) ENGINE=InnoDB DEFAULT CHARSET=utf8;
###
ALTER TABLE `category`
  ADD PRIMARY KEY (`id`),
  ADD UNIQUE KEY `category-id-uindex` (`id`),
  ADD KEY `category-parent-index` (`parent`),
  ADD KEY `category-status-index` (`status`);
###
ALTER TABLE `chat`
  ADD PRIMARY KEY (`id`),
  ADD UNIQUE KEY `chat-id-uindex` (`id`);
###
ALTER TABLE `comments`
  ADD PRIMARY KEY (`id`),
  ADD UNIQUE KEY `comments-id-uindex` (`id`),
  ADD KEY `comments-content-index` (`content`);
###
ALTER TABLE `content`
  ADD PRIMARY KEY (`id`),
  ADD UNIQUE KEY `content-id-uindex` (`id`),
  ADD KEY `content-category-index` (`category`),
  ADD KEY `content-user-index` (`user`);
###
ALTER TABLE `formdata`
  ADD PRIMARY KEY (`id`);
###
ALTER TABLE `logs`
  ADD PRIMARY KEY (`id`),
  ADD UNIQUE KEY `logs-id-uindex` (`id`),
  ADD KEY `logs-user-index` (`user`);
###
ALTER TABLE `messages`
  ADD PRIMARY KEY (`id`),
  ADD UNIQUE KEY `messages-id-uindex` (`id`);
###
ALTER TABLE `pages`
  ADD PRIMARY KEY (`id`),
  ADD UNIQUE KEY `pages-id-uindex` (`id`),
  ADD KEY `pages-parent-index` (`parent`);
###
ALTER TABLE `statistics`
  ADD PRIMARY KEY (`id`);
###
ALTER TABLE `subscriptions`
  ADD PRIMARY KEY (`id`),
  ADD UNIQUE KEY `subscriptions-id-uindex` (`id`);
###
ALTER TABLE `tags`
  ADD PRIMARY KEY (`id`),
  ADD UNIQUE KEY `tags-id-uindex` (`id`);
###
ALTER TABLE `users`
  ADD PRIMARY KEY (`id`),
  ADD UNIQUE KEY `users-id-uindex` (`id`);
###
ALTER TABLE `category`
  MODIFY `id` int(11) NOT NULL AUTO_INCREMENT;
###
ALTER TABLE `chat`
  MODIFY `id` int(11) NOT NULL AUTO_INCREMENT;
###
ALTER TABLE `comments`
  MODIFY `id` int(11) NOT NULL AUTO_INCREMENT;
###
ALTER TABLE `content`
  MODIFY `id` int(11) NOT NULL AUTO_INCREMENT;
###
ALTER TABLE `formdata`
  MODIFY `id` int(11) NOT NULL AUTO_INCREMENT;
###
ALTER TABLE `logs`
  MODIFY `id` int(11) NOT NULL AUTO_INCREMENT;
###
ALTER TABLE `messages`
  MODIFY `id` int(11) NOT NULL AUTO_INCREMENT;
###
ALTER TABLE `pages`
  MODIFY `id` int(11) NOT NULL AUTO_INCREMENT;
###
ALTER TABLE `statistics`
  MODIFY `id` int(11) NOT NULL AUTO_INCREMENT;
###
ALTER TABLE `subscriptions`
  MODIFY `id` int(11) NOT NULL AUTO_INCREMENT;
###
ALTER TABLE `tags`
  MODIFY `id` int(11) NOT NULL AUTO_INCREMENT;
###
ALTER TABLE `users`
  MODIFY `id` int(11) NOT NULL AUTO_INCREMENT;
